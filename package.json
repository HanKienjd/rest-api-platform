    "@types/node": "^18.17.0",
    "autoprefixer": "^10.4.14",
    "element-plus": "^2.3.8",
    "eslint": "^8.45.0",
    "nuxt": "^3.6.5",
    "nuxt-icon": "^0.4.2",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
